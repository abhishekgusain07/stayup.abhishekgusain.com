{
  "name": "@stayup/shared-types",
  "version": "1.0.0",
  "description": "Shared Zod schemas and TypeScript types for StayUp monitoring application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "monitoring",
    "zod",
    "typescript",
    "schemas",
    "types"
  ],
  "author": "StayUp Team",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/",
    "src/"
  ]
}